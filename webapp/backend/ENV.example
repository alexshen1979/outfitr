# Backend Environment Variables

Copy this file to `.env` and update the values:

NODE_ENV=development
PORT=3001

# Database Configuration
DB_HOST=your-database-host
DB_PORT=3306
DB_USER=your-database-user
DB_PASSWORD=your-database-password
DB_NAME=your-database-name

# JWT Configuration
JWT_SECRET=your-secret-key-change-in-production

# AI Service Provider (replicate | piccopilot | aliyun | default)
AI_PROVIDER=replicate

# Replicate API Configuration (推荐，性价比高)
REPLICATE_API_TOKEN=your-replicate-api-token
REPLICATE_TIMEOUT=120000

# Pic Copilot API Configuration (备选，目前免费但无公开API)
PIC_COPILOT_API_KEY=your-piccopilot-api-key
PIC_COPILOT_API_ENDPOINT=https://api.piccopilot.com/v1
PIC_COPILOT_TIMEOUT=60000

# 通用 AI API Configuration (备选)
AI_API_KEY=your-ai-api-key
AI_API_ENDPOINT=https://api.example.com
AI_TIMEOUT=60000

# File Upload Configuration
UPLOAD_DIR=./uploads

